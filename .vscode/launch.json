{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Iniciar NestJS",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/dist/main.js", // Caminho do arquivo compilado
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "preLaunchTask": "tsc: build - tsconfig.json"
        }
    ]
}
